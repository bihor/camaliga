{
	"name" : "quizpalme/camaliga",
	"type" : "typo3-cms-extension",
	"description" : "A carousel/gallery/map/list extension that can use the TYPO3 categories and different jQuery-plugins like Slick or Isotope. Bootstrap 5 support. Indexer for ke_search.",
	"authors" : [
		{
			"name" : "Kurt Gusbeth",
			"role" : "Developer"
		}
	],
	"require" : {
		"typo3/cms-core" : "~12.4.0 || ~13.4.4"
	},
	"autoload" : {
		"psr-4" : {
			"Quizpalme\\Camaliga\\" : "Classes"
		}
	},
	"replace" : {
		"typo3-ter/camaliga" : "self.version"
	},
	"homepage" : "https://github.com/bihor/camaliga",
	"license" : "GPL-3.0-or-later",
	"keywords" : [
		"slider",
		"carousel",
		"maps",
		"gallery"
	],
	"extra": {
		"typo3/cms": {
			"extension-key": "camaliga"
		}
	}
}
